{
  "name": "Fourier",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "watch": {
    "makehtml": {
      "patterns": [
        "content/*.md"
      ],
      "extensions": "md"
    },
    "webpack": "js/**/*.js",
    "css": "css/styles.css",
    "img": "img/*"
  },
  "scripts": {
    "start": "npm run build && reload -b -d build",
    "webpack": "webpack || exit 1",
    "makehtml": "node script/make-html",
    "css": "cp -r css build/",
    "img": "cp -r img build/",
    "preview": "cp preview.png build/",
    "build": "npm run webpack && npm run makehtml && npm run css && npm run img && npm run preview",
    "test": "jest",
    "watch": "npm-watch",
    "reload": "reload -b -d build"
  },
  "author": "Jezzamon",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.8.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.7.0",
    "http-server": "^0.11.1",
    "jest": "^24.8.0",
    "mustache": "^3.0.1",
    "npm-watch": "^0.3.0",
    "regenerator-runtime": "^0.12.1",
    "reload": "^2.3.1",
    "showdown": "^1.9.0",
    "webpack": "^3.12.0"
  },
  "dependencies": {
    "browser-image-compression": "0.0.3",
    "fft.js": "^4.0.3",
    "matrix-multiplication": "^0.5.2"
  }
}
